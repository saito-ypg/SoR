{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "array",
	"items": {
		"type": "object",
		"properties": {
			"name": {
				"type": "string"
			},
			"params": {
				"type": "object",
				"properties": {
					"param1": {
						"type": "number"
					},
					"param2": {
						"type": "number"
					}
				},
				"required": [ "param1", "param2" ]
			}
		},
		"required": [ "name", "params" ]
	}
}