{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "Games": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "wave_id": {
            "type": "integer"
          },
          "stage": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "spawn_time": {
                  "type": "number"
                },
                "enemy_type": {
                  "type": "string"
                },
                "is_boss": {
                  "type": "boolean"
                }
              },
              "required": [ "spawn_time", "enemy_type", "is_boss" ]
            }
          }
        },
        "required": [ "wave_id", "stage" ]
      }
    }
  },
  "required": [ "Games" ]
}
